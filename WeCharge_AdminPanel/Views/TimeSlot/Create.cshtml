@{
    Layout = "/Views/Shared/_AdminLayout.cshtml";
}
@model TimeSlotViewModel
<div class="content">
    <div class="container-fluid">
        <div class="card-header">
            <h3 class="card-title"><b>Add New TimeSlot<b></h3>
        </div>
        <div class="card-body">
            <form asp-action="CreateTimeSlot" asp-controller="TimeSlot" autocomplete="off">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="TIME_SLOT_FROM">Time Slot From</label>
                    <input id="TIME_SLOT_FROM" asp-for="TIME_SLOT_FROM" type="time" class="form-control" id="name" placeholder="Enter your Full Name">
                    <span asp-validation-for="TIME_SLOT_FROM" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="TIME_SLOT_TO">Time Slot From</label>
                    <input id="TIME_SLOT_TO" asp-for="TIME_SLOT_TO" type="time" class="form-control" id="name" placeholder="Enter your Full Name">
                    <span asp-validation-for="TIME_SLOT_TO" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <div class="col-sm-4 mt-2">
                        <input id="btnSubmit" type="submit" class="btn btn-primary" />
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
<script src="~/JS/Jquery.min.js"></script>
<script>
    $(document).ready(function () {
        btnSubmit
        $('#btnSubmit').prop('disabled', true);
    });

    $("#TIME_SLOT_FROM").change(function () {
        var fromVal = $("#TIME_SLOT_FROM").val();
        var toVal = $("#TIME_SLOT_TO").val();
        debugger;
        if (fromVal != "" && toVal != "") {
            $('#btnSubmit').prop('disabled', false);
        }

    });

    $("#TIME_SLOT_TO").change(function () {
        var fromVal = $("#TIME_SLOT_FROM").val();
        var toVal = $("#TIME_SLOT_TO").val();
        if (fromVal != "" && toVal != "") {
            $('#btnSubmit').prop('disabled', false);
        }
    });
</script>